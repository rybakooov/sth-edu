# editorconfig.org
root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
quote_type = single
max_line_length = 100
spaces_around_operators = true
spaces_around_brackets = none

[*.md]
trim_trailing_whitespace = false

